{
  "DIAL": {
    "state": {
      "type": "number",
      "constraints": {
        "min": 0,
        "max": 100
      }
    },
    "events": [
      "increment",
      "decrement"
    ]
  },
  "BUTTON": {
    "state": {
      "type": "boolean"
    },
    "events": [
      "press",
      "release"
    ]
  }
}